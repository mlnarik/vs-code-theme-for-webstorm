<keymap version="1" name="VSCode Keymap" parent="VSCode">
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl p" />
    <keyboard-shortcut first-keystroke="f1" />
  </action>
  <action id="GotoNextError">
    <keyboard-shortcut first-keystroke="shift f8" />
  </action>
  <action id="GotoPreviousError" />
  <action id="Resume">
    <keyboard-shortcut first-keystroke="f8" />
  </action>
  <action id="SaveAll">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="s" />
    <keyboard-shortcut first-keystroke="ctrl s" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
  </action>
</keymap>